{
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/public/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "buildComand": "npm run build",
      "outputDirectory": "public"
    }
  ]
}
